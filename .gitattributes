# LINGUIST OPTIMIZATION - FORCE R DOMINANCE
*.ipynb linguist-language=R
*.ipynb linguist-detectable=true

# Force R dominance by hiding generated/web exports
"web/**" linguist-vendored
"web/**" linguist-detectable=false
"docs/**" linguist-documentation
"docs/**" linguist-detectable=false
"Mega/**" linguist-vendored
"Mega/**" linguist-detectable=false
"Certificates/**" linguist-vendored
"Certificates/**" linguist-detectable=false

# Keep R scripts as the only primary detectable logic
*.R linguist-detectable=true
*.r linguist-detectable=true
